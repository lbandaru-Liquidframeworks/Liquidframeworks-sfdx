<apex:page controller="FXBaseController" extensions="FXRemoter" sidebar="false" showChat="false">
<input style="opacity: 0.0; height: 0px" type="submit" onclick="return false;" />

<link rel="stylesheet" href="https://cdn.fieldfx.com/{!$Api.Session_ID}/{!namespaceWithoutUnderscores + '__'}/{!fxCustomSettings.CDN_IIB_Resource_URL}/stylescss.css?orgid={!$organization.id}&orgname={!$organization.name}&loginname={!$User.UserName}&userid={!$User.id}&username={!$User.FirstName + ' ' + $User.LastName}" />

<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/themes/redmond/jquery-ui.css" />
