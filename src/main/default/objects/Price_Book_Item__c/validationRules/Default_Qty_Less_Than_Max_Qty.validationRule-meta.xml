<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Default_Qty_Less_Than_Max_Qty</fullName>
    <active>true</active>
    <errorConditionFormula>!ISBLANK(Default_Quantity__c) &amp;&amp; !ISBLANK(Maximum_Quantity__c) &amp;&amp; Default_Quantity__c  >  Maximum_Quantity__c</errorConditionFormula>
    <errorMessage>The Default Quantity must be less than or equal to the Maximum Quantity.</errorMessage>
</ValidationRule>
