<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_Qty_Cannot_Exceed_Max_Qty</fullName>
    <active>true</active>
    <errorConditionFormula>!ISBLANK(Minimum_Quantity__c) &amp;&amp; !ISBLANK(Maximum_Quantity__c) &amp;&amp; Minimum_Quantity__c  >  Maximum_Quantity__c</errorConditionFormula>
    <errorMessage>Minimum Quantity must be either less than or equal to Maximum Quantity or set to nothing.</errorMessage>
</ValidationRule>
