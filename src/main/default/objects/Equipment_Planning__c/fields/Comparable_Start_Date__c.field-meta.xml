<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Comparable_Start_Date__c</fullName>
    <description>Used for internal date comparison with the Job Scheduling/Whiteboard feature.</description>
    <externalId>false</externalId>
    <formula>VALUE(TEXT(YEAR(DATEVALUE(IF(ISBLANK(Planned_Start__c),Planned_End__c,Planned_Start__c))))+LPAD(TEXT(MONTH(DATEVALUE(IF(ISBLANK(Planned_Start__c),Planned_End__c,Planned_Start__c)))),2,'0')+LPAD(TEXT(DAY(DATEVALUE(IF(ISBLANK(Planned_Start__c),Planned_End__c,Planned_Start__c)))),2,'0'))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Date and time the equipment is due to start being used on the job.</inlineHelpText>
    <label>Comparable_Start_Date</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
